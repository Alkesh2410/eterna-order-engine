{
  "info": {
    "name": "Eterna Order Execution Engine API",
    "description": "API collection for order execution engine with DEX routing",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Health Check",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/health",
          "host": ["{{base_url}}"],
          "path": ["health"]
        }
      }
    },
    {
      "name": "Submit Market Order",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"type\": \"market\",\n  \"tokenIn\": \"SOL\",\n  \"tokenOut\": \"USDC\",\n  \"amountIn\": \"100\",\n  \"slippageTolerance\": 1.0\n}"
        },
        "url": {
          "raw": "{{base_url}}/api/orders/execute",
          "host": ["{{base_url}}"],
          "path": ["api", "orders", "execute"]
        }
      }
    },
    {
      "name": "Submit Order with Min Amount",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"type\": \"market\",\n  \"tokenIn\": \"SOL\",\n  \"tokenOut\": \"USDC\",\n  \"amountIn\": \"50\",\n  \"slippageTolerance\": 0.5,\n  \"minAmountOut\": \"49\"\n}"
        },
        "url": {
          "raw": "{{base_url}}/api/orders/execute",
          "host": ["{{base_url}}"],
          "path": ["api", "orders", "execute"]
        }
      }
    },
    {
      "name": "Get Order Details",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/orders/{{order_id}}",
          "host": ["{{base_url}}"],
          "path": ["api", "orders", "{{order_id}}"]
        }
      }
    },
    {
      "name": "List All Orders",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/orders",
          "host": ["{{base_url}}"],
          "path": ["api", "orders"]
        }
      }
    },
    {
      "name": "List Orders by Status",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/orders?status=confirmed&limit=10",
          "host": ["{{base_url}}"],
          "path": ["api", "orders"],
          "query": [
            {
              "key": "status",
              "value": "confirmed"
            },
            {
              "key": "limit",
              "value": "10"
            }
          ]
        }
      }
    },
    {
      "name": "Get Queue Statistics",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/api/queue/stats",
          "host": ["{{base_url}}"],
          "path": ["api", "queue", "stats"]
        }
      }
    },
    {
      "name": "Submit Multiple Orders (Batch)",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"type\": \"market\",\n  \"tokenIn\": \"SOL\",\n  \"tokenOut\": \"USDC\",\n  \"amountIn\": \"25\"\n}"
        },
        "url": {
          "raw": "{{base_url}}/api/orders/execute",
          "host": ["{{base_url}}"],
          "path": ["api", "orders", "execute"]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:3000",
      "type": "string"
    },
    {
      "key": "order_id",
      "value": "",
      "type": "string"
    }
  ]
}

